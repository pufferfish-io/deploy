environments:
  production:
    values: []

---
repositories:
  - name: bitnami
    url: https://charts.bitnami.com/bitnami

releases:
  - name: kafka
    namespace: default
    chart: bitnami/kafka
    version: 28.0.4
    values: [kafka-values.yaml]
    set:
      - name: kraft.clusterId
        value: {{ env "KAFKA_CLUSTER_ID" }}